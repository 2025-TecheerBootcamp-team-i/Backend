{
  "title": "RabbitMQ Metrics",
  "tags": ["rabbitmq", "message-queue", "celery"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Queue Messages (Ready)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum(rabbitmq_queue_messages_ready) or vector(0)",
          "legendFormat": "Total Ready",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Messages"},
        {"format": "short"}
      ]
    },
    {
      "id": 2,
      "title": "Queue Messages (Unacked)",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "sum(rabbitmq_queue_messages_unacked) or vector(0)",
          "legendFormat": "Total Unacked",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "short", "label": "Messages"},
        {"format": "short"}
      ]
    },
    {
      "id": 3,
      "title": "Message Publish Rate",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_queue_messages_published_total[5m])) or vector(0)",
          "legendFormat": "Published/sec",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "ops", "label": "Messages/sec"},
        {"format": "short"}
      ]
    },
    {
      "id": 4,
      "title": "Message Deliver Rate",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_global_messages_delivered_total[5m])) or vector(0)",
          "legendFormat": "Delivered/sec",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "ops", "label": "Messages/sec"},
        {"format": "short"}
      ]
    },
    {
      "id": 5,
      "title": "Connections",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "rabbitmq_connections or vector(0)",
          "legendFormat": "Connections",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "short"
      }
    },
    {
      "id": 6,
      "title": "Channels",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 16},
      "targets": [
        {
          "expr": "rabbitmq_channels or vector(0)",
          "legendFormat": "Channels",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "short"
      }
    },
    {
      "id": 7,
      "title": "Queues",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "rabbitmq_queues or vector(0)",
          "legendFormat": "Queues",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "short"
      }
    },
    {
      "id": 8,
      "title": "Consumers",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 16},
      "targets": [
        {
          "expr": "rabbitmq_global_consumers or vector(0)",
          "legendFormat": "Consumers",
          "refId": "A"
        }
      ],
      "options": {
        "unit": "short"
      }
    },
    {
      "id": 9,
      "title": "Memory Usage",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
      "targets": [
        {
          "expr": "(rabbitmq_process_resident_memory_bytes / rabbitmq_resident_memory_limit_bytes * 100) or vector(0)",
          "legendFormat": "Memory Usage %",
          "refId": "A"
        }
      ],
      "yaxes": [
        {"format": "percent", "label": "Memory %", "max": 100},
        {"format": "short"}
      ]
    },
    {
      "id": 10,
      "title": "Messages Received vs Acknowledged",
      "type": "graph",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
      "targets": [
        {
          "expr": "sum(rate(rabbitmq_global_messages_received_total[5m])) or vector(0)",
          "legendFormat": "Received/sec",
          "refId": "A"
        },
        {
          "expr": "sum(rate(rabbitmq_global_messages_acknowledged_total[5m])) or vector(0)",
          "legendFormat": "Acknowledged/sec",
          "refId": "B"
        }
      ],
      "yaxes": [
        {"format": "ops", "label": "Messages/sec"},
        {"format": "short"}
      ]
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
  }
}
